{
  "manifest_version": 2,
  "name": "Commander",
  "version": "0.8",

  "description": "Command launcher to control websites and the browser.",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["./content.js"]
    }
  ],
  "background": {
    "scripts": ["./background.js"]
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "activeTab",
    "bookmarks"
  ]

}
